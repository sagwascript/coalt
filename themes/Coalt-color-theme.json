	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0088ff"
			}
		},
		{
			"name": "Constant",
			"scope": "constant",
			"settings": {
				"foreground": "#ff628c"
			}
		},
		{
			"name": "Entity",
			"scope": "entity",
			"settings": {
				"foreground": "#ffc600"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#f44542"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword, storage.type.class.js",
			"settings": {
				"foreground": "#ff9d00"
			}
		},
		{
			"name": "Meta",
			"scope": "meta",
			"settings": {
